<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="es.humarbean.gespagos">

    <application
        android:name=".application.GesPagosApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".views.MateListActivity"
            android:label="Mate List ">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".views.MateEditorActivity"
            android:label="Mate Detail"
            android:parentActivityName=".views.MateListActivity"/>
        <activity android:name=".views.AndroidDatabaseManager"
            android:theme="@style/Theme.AppCompat.Light"/>
    </application>

</manifest>
